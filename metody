from random import random

def add(a,b):
    return a + b

sum_two=add(11,21)
print(sum_two)


# mnożenie

def multiply(a,b):
    return a * b  # metoda oddaje wynik

quotient = multiply(11,21)    # jakas zmienna przyjmuje wynik metody powyzej
print(quotient)

def volume(a,b,c):
    return a * b * c  # metoda oddaje wynik

volume_cuboid = volume(11,21,13)    # jakas zmienna przyjmuje wynik metody powyzej
print(volume_cuboid)


def volume(a,b,c):
    return a * b * c  # metoda oddaje wynik

volume_cuboid = volume(3,5,112)    # jakas zmienna przyjmuje wynik metody powyzej
print(volume_cuboid)

pi=3.14
def volume_b(r):
    return 0.75 * pi* r**3  # metoda oddaje wynik

volume_ball = volume_b(10)    # jakas zmienna przyjmuje wynik metody powyzej
print(volume_ball)

if volume_cuboid > volume_ball:
        print ("Większa jest kula")
else:
        print ("Większy jest prostopadłościan")


